# ================================
# PDF Industrial Pipeline - Client Frontend Development Environment
# ================================

# Environment
NODE_ENV=development
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:9082

# Supabase Configuration (for authentication and user management)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database Configuration - Not used in client frontend, but kept for compatibility
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pdf_pipeline_dev

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false